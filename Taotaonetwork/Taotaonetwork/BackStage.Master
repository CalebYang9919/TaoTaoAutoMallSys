                    //    }
                    //});

                    new Chart(document.getElementById("myChart-nav").getContext('2d'), {
                        type: 'doughnut',
                        data: {
                            labels: ["<%=shu[0] %>", "<%=shu[1] %>", "<%=shu[2] %>", "<%=shu[3] %>", "<%=shu[4] %>"],
                            datasets: [{
                                backgroundColor: [
